<h1>{{noYear tournament.name}}</h1><div class="row clearfix s_mb_20"><div class="col-md-8 column"><div class="row clearfix"><div class="col-md-6 column"><div class="row"><div class="col-md-12 s_mb_20"><div class="panel s_m_0"><div class="panel__header"><i class="fa fa-list-ol s_mr_10"></i> Таблица</div><div class="panel__body s_p_0 s_pb_5">{{> table table=this.globals.table league=globals.league tournament=tournament tableFull=false showLogo=true}}</div></div><div class="link_type_full"><span class="text_color_orange">Полная таблица</span> <span class="pull-right"><a href="/{{globals.league.slug}}/tournaments/{{tournament.slug}}/table"><i class="fa fa-arrow-circle-o-right"></i></a></span></div></div></div></div><div class="col-md-6 column"><div class="row clearfix"><div class="col-md-12 column"><div class="tabbable"><ul class="nav nav-tabs"><li class="tab__title tab__title_size_half active"><a href="#strikers" data-toggle="tab">Бомбардиры</a></li><li class="tab__title tab__title_size_half"><a href="#assists" data-toggle="tab">Ассистенты</a></li></ul><div class="tab-content"><div class="tab-pane active" id="strikers"><div class="panel s_m_0"><div class="panel__body s_p_0 s_pb_5">{{> strikers stats=this.globals.stats.goals emptyText='Голов ещё не забито'}}</div></div></div><div class="tab-pane" id="assists"><div class="panel s_m_0"><div class="panel__body s_p_0 s_pb_5">{{> assists stats=this.globals.stats.assists emptyText='Никто не отдавал голевых'}}</div></div></div></div></div><div class="link_type_full"><span class="text_color_orange">Статистика</span> <span class="pull-right"><a href="/{{globals.league.slug}}/tournaments/{{tournament.slug}}/stats"><i class="fa fa-arrow-circle-o-right"></i></a></span></div></div></div></div></div>{{#if central}}<div class="row clearfix s_mb_20"><div class="column col-md-12"><div class="match-main">{{#ifCond central.imageHome '&&' central.imageAway}}<div class="match-main_image match-main_image_home" style="background-image: url('{{central.imageHome}}')"></div><div class="match-main_image match-main_image_away" style="background-image: url('{{central.imageAway}}')"></div>{{else}}<div class="match-main_image match-main_image_home" style="background-image: url('http://amateurs.io/api/img/teams/{{central.game.teams.[0].name}}.png')"></div><div class="match-main_image match-main_image_away" style="background-image: url('http://amateurs.io/api/img/teams/{{central.game.teams.[1].name}}.png')"></div>{{/ifCond}}<div class="match-main_info"><div class="match-main_teams"><a href="/{{globals.league.slug}}/tournaments/{{globals.tournament.slug}}/matches/{{central.game._id}}"><span class="match-main_team match-main_team_home">{{central.game.teams.[0].name}}</span> <span class="match-main_vs">vs</span> <span class="match-main_team match-main_team_away">{{central.game.teams.[1].name}}</span></a></div><div class="match-main_hot"><i class="flaticon-flaming"></i></div><div class="match-main_place">{{#unless central.game.score.ft}} {{#if central.game.placeName}} <i class="fa fa-location-arrow"></i> <a class="text_color_light" href="/{{globals.league.slug}}/tournaments/{{globals.tournament.slug}}/fields/{{slugify central.game.placeName}}"><span class="match-main_field">{{central.game.placeName}}</span></a> {{/if}} {{else}} <a class="text_color_orange text_size_48" href="/{{globals.league.slug}}/tournaments/{{globals.tournament.slug}}/matches/{{central.game._id}}">{{central.game.score.ft.[0]}}:{{central.game.score.ft.[1]}}</a> {{/unless}} <span class="match-main_time text_color_light">{{central.game.tourNumber}} ТУР</span> {{#if central.game.dateTime}} <span class="match-main_time">{{date central.game.dateTime}}</span> <span class="match-main_time">{{central.game.time}}</span> {{/if}}</div></div><!--<svg width="0" height="0">--><!--<clipPath id="clip-match-main_image_home">--><!--<polygon points="300 300,360 150,300 0,0 0,0 300">--><!--</polygon>--><!--</clipPath>--><!--<clipPath id="clip-match-main_image_away">--><!--<polygon points="0 150,60 300,360 300,360 0,60 0">--><!--</polygon>--><!--</clipPath>--><!--<clipPath id="clip-match-main_info">--><!--<polygon points="60 150,0 300,200 300,140 150,200 0,0 0">--><!--</polygon>--><!--</clipPath>--><!--</svg>--></div></div></div>{{/if}} {{#if previews.length}}<div class="row clearfix s_mb_20"><div class="col-md-12 column"><div class="panel panel_type_article s_m_0"><div class="panel__header"><h4 class="s_m_0"><i class="fa fa-eye s_mr_5"></i> Последние анонсы</h4></div><div class="{{#gt previews.length 1}}owl-carousel js-carousel{{/gt}}" data-items="1" data-pagination="true" data-prev="Назад" data-next="Дальше">{{#each previews}}<div class="panel__body reading">{{> article_preview article=this league=../globals.league tournament=../globals.tournament}}</div>{{/each}}</div></div><!--<div class="link_type_full">--><!--<span class="text_color_orange">Все обзоры</span>--><!--<span class="pull-right">--><!--<a href="/leagues/{{tournament.slug}}/table"><i class="fa fa-arrow-circle-o-right"></i></a>--><!--</span>--><!--</div>--></div></div>{{/if}} {{#if reviews}}<div class="row clearfix s_mb_20"><div class="col-md-12 column"><div class="panel panel_type_article s_m_0"><div class="panel__header"><h4 class="s_m_0"><i class="fa fa-eye s_mr_5"></i> Последние обзоры</h4></div><div class="{{#gt reviews.length 1}}owl-carousel owl-theme js-carousel{{/gt}}" data-items="1" data-pagination="true" data-prev="Назад" data-next="Дальше">{{#each reviews}}<div class="panel__body reading">{{> article_review article=this league=../globals.league tournament=../globals.tournament}}</div>{{/each}}</div></div></div></div>{{/if}} {{#if photos}}<div class="row clearfix s_mb_20"><div class="col-md-12 column"><div class="panel panel_type_photo panel_type_photo-preview s_m_0"><div class="panel__header"><h4 class="s_m_0"><i class="fa fa-camera-retro s_mr_5"></i> Последние фотоотчеты</h4></div>{{#each photos}}<div class="panel__body">{{> tournament_photos photos=photos game=game league=../globals.league tournament=../globals.tournament}}</div>{{/each}}</div></div></div>{{/if}}</div><div class="col-md-4 column sidebar"><!--<div class="panel s_m_0">--><!--<div class="panel-body">--><!--<form role="form" class="form form_type_signin" action="/login" method="post">--><!--<div class="form-group form-group_icon_left">--><!--<i class="fa fa-user"></i>--><!--<input type="email" class="form-control" id="signin-email" placeholder="myname@amateurs.io">--><!--</div>--><!--<div class="form-group form-group_icon_left">--><!--<i class="fa fa-key"></i>--><!--<input type="password" class="form-control" id="signin-password" placeholder="password">--><!--</div>--><!--<button type="submit" class="btn btn-default">Войти</button>--><!--</form>--><!--</div>--><!--</div>--><!--<div class="link_type_full s_mb_20">--><!--<span class="text_color_orange">Регистрация</span>--><!--<span class="pull-right">--><!--<a href="/signup"><i class="fa fa-arrow-circle-o-right"></i></a>--><!--</span>--><!--</div>--><div class="panel panel_type_fixture s_m_0"><div class="panel__header"><i class="fa fa-calendar s_mr_10"></i> Последние матчи</div><div class="panel__body">{{> 'fixture' fixture=globals.recent emptyText='Ещё нет сыгранных матчей' }}</div></div><div class="link_type_full s_mb_10"><span class="text_color_orange">Календарь</span> <span class="pull-right"><a href="/{{globals.league.slug}}/tournaments/{{tournament.slug}}/fixture"><i class="fa fa-arrow-circle-o-right"></i></a></span></div><div class="panel panel_type_fixture s_m_0"><div class="panel__header"><i class="fa fa-calendar s_mr_10"></i> Ближайшие матчи</div><div class="panel__body">{{> 'fixture' fixture=globals.comming emptyText='Нет запланированных матчей' }}</div></div><div class="link_type_full s_mb_10"><span class="text_color_orange">Календарь</span> <span class="pull-right"><a href="/{{globals.league.slug}}/tournaments/{{tournament.slug}}/fixture"><i class="fa fa-arrow-circle-o-right"></i></a></span></div>{{#notEmptyObject globals.news}}<div class="panel s_m_0"><div class="panel__header"><i class="fa fa-quote-right text_size_16 s_mr_10"></i> Новости</div><div class="panel__body">{{> news news=globals.news}}</div></div><div class="link_type_full s_mb_10"><span class="text_color_orange">Все новости</span> <span class="pull-right"><a href="/{{globals.league.slug}}/tournaments/{{tournament.slug}}/news"><i class="fa fa-arrow-circle-o-right"></i></a></span></div>{{/notEmptyObject}}<!-- CONTACTS -->{{#if globals.tournament.contacts.length}}<div class="panel s_m_0"><div class="panel__header"><i class="fa fa-phone text_size_16 s_mr_10"></i> Контакты</div><div class="panel__body">{{> contacts contacts=globals.tournament.contacts}}</div></div><div class="link_type_full s_mb_10"><span class="text_color_orange">Все контакты</span> <span class="pull-right"><a href="/{{globals.league.slug}}/contacts"><i class="fa fa-arrow-circle-o-right"></i></a></span></div>{{/if}}<!-- PARTNERS --><div class="panel s_m_0"><div class="panel__header"><i class="fa fa-suitcase text_size_16 s_mr_10"></i> Партнеры</div><div class="panel__body">{{> partners}}</div></div></div></div>