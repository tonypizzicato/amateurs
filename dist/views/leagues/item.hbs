<h1>{{league.name}}</h1><div class="row clearfix s_mb_20"><div class="col-md-8 column"><div class="row clearfix"><div class="col-md-12"><div class="accordion js-accordion" data-opened="0">{{#each league.countries}}<div class="accordion__item{{#if @first}} accordion__item_opened{{/if}} js-accordion-item"><a class="accordion__header js-accordion-control" href="#"><div class="flag__wrapper s_mr_20"><div class="flag flag_country_{{slug}}"></div></div>{{~name~}} <span class="st-arrow">Открыть/Закрыть</span></a><div class="accordion__content js-accordion-content">{{~# each tournaments~}}<div class="tabbable s_mb_40"><div class="tab-pane{{#if @first}} active{{/if}}" id="tab-{{slug}}"><div class="tabbable s_mb_40"><ul class="nav nav-tabs js-tabs"><li class="tab__title active tab__title_size_4"><a href="#tab-{{slug}}-table" data-toggle="tab">{{noYear name}}</a></li><li class="tab__title tab__title_size_4"><a href="#tab-{{slug}}-recent" class="js-tab-lazy" data-toggle="tab" data-lazy="true" data-league="{{../../league.slug}}" data-name="{{slug}}" data-route="recent">Последние матчи</a></li><li class="tab__title tab__title_size_4"><a href="#tab-{{slug}}-comming" class="js-tab-lazy" data-toggle="tab" data-lazy="true" data-league="{{../../league.slug}}" data-name="{{slug}}" data-route="comming">Ближайшие матчи</a></li><li class="tab__title tab__title_size_4"><a href="#tab-{{slug}}-stats" class="js-tab-lazy" data-toggle="tab" data-lazy="true" data-league="{{../../league.slug}}" data-name="{{slug}}" data-route="stats">Статистика</a></li></ul><div class="tab-content"><div class="tab-pane active" id="tab-{{slug}}-table"><div class="panel s_m_0"><div class="panel__body">{{#each tables}} {{#gt ../tables.length 1}}<p class="group-name">{{stageName}}</p>{{/gt}}<div class="{{#unless @last}}s_mb_20{{/unless}}">{{> table table=this league=../../../league tournament=../this tableFull=true showLogo=true}}</div>{{/each}}</div></div><div class="link_type_full s_mb_10"><span class="text_color_orange">Таблица</span> <span class="pull-right"><a href="/{{../../league.slug}}/tournaments/{{slug}}/table"><i class="fa fa-arrow-circle-o-right"></i></a></span></div></div><div class="tab-pane" id="tab-{{slug}}-recent"><div class="panel s_m_0"><div class="panel__body"><div class="loader"><div class="loader__ball"></div><p class="loader__text">Загрузка</p></div></div></div><div class="link_type_full s_mb_10"><span class="text_color_orange">Календарь</span> <span class="pull-right"><a href="/{{../../league.slug}}/tournaments/{{slug}}/fixture"><i class="fa fa-arrow-circle-o-right"></i></a></span></div></div><div class="tab-pane" id="tab-{{slug}}-comming"><div class="panel s_m_0"><div class="panel__body"><div class="loader"><div class="loader__ball"></div><p class="loader__text">Загрузка</p></div></div></div><div class="link_type_full s_mb_10"><span class="text_color_orange">Календарь</span> <span class="pull-right"><a href="/{{../../league.slug}}/tournaments/{{slug}}/fixture"><i class="fa fa-arrow-circle-o-right"></i></a></span></div></div><div class="tab-pane" id="tab-{{slug}}-stats"><div class="panel s_m_0"><div class="panel__body"><div class="loader"><div class="loader__ball"></div><p class="loader__text">Загрузка</p></div></div></div><div class="link_type_full s_mb_10"><span class="text_color_orange">Статистика</span> <span class="pull-right"><a href="/{{../../league.slug}}/tournaments/{{slug}}/stats"><i class="fa fa-arrow-circle-o-right"></i></a></span></div></div></div></div></div></div>{{/each}}</div></div>{{/each}}</div></div></div></div><div class="col-md-4 column sidebar"><!--<div class="panel s_m_0">--><!--<div class="panel-body">--><!--<form role="form" class="form form_type_signin" action="/login" method="post">--><!--<div class="form-group form-group_icon_left">--><!--<i class="fa fa-user"></i>--><!--<input type="email" class="form-control" id="signin-email" placeholder="myname@amateurs.io">--><!--</div>--><!--<div class="form-group form-group_icon_left">--><!--<i class="fa fa-key"></i>--><!--<input type="password" class="form-control" id="signin-password" placeholder="password">--><!--</div>--><!--<button type="submit" class="btn btn-default">Войти</button>--><!--</form>--><!--</div>--><!--</div>--><!--<div class="link_type_full s_mb_20">--><!--<span class="text_color_orange">Регистрация</span>--><!--<span class="pull-right">--><!--<a href="/signup"><i class="fa fa-arrow-circle-o-right"></i></a>--><!--</span>--><!--</div>--><div class="panel s_mb_20"><div class="panel__header"><i class="fa fa-trophy text_size_16 s_mr_10"></i> Турниры</div><div class="panel__body">{{#each league.countries}}<h5 class="text_color_orange s_mt_0 s_mb_10">{{name}}</h5><ul>{{#each tournaments}}<li class="s_ml_20 s_mb_5"><a href="/{{../../globals.league.slug}}/tournaments/{{slug}}">{{noYear name}}</a></li>{{/each}}</ul>{{/each}}</div></div><!-- NEWS -->{{#notEmptyObject globals.news}}<div class="panel s_m_0"><div class="panel__header"><i class="fa fa-quote-right text_size_16 s_mr_10"></i> Новости</div><div class="panel__body">{{> news news=globals.news}}</div></div><div class="link_type_full s_mb_10"><span class="text_color_orange">Все новости</span> <span class="pull-right"><a href="/{{globals.league.slug}}/news"><i class="fa fa-arrow-circle-o-right"></i></a></span></div>{{/notEmptyObject}}<!-- CONTACTS -->{{#if globals.contacts.length}}<div class="panel s_m_0"><div class="panel__header"><i class="fa fa-phone text_size_16 s_mr_10"></i> Контакты</div><div class="panel__body">{{> contacts contacts=globals.contacts}}</div></div><div class="link_type_full s_mb_10"><span class="text_color_orange">Все контакты</span> <span class="pull-right"><a href="/{{globals.league.slug}}/contacts"><i class="fa fa-arrow-circle-o-right"></i></a></span></div>{{/if}}<!-- PARTNERS --><div class="panel s_m_0"><div class="panel__header"><i class="fa fa-suitcase text_size_16 s_mr_10"></i> Партнеры</div><div class="panel__body">{{> partners}}</div></div></div></div>