<table class="table table_type_table s_m_0">
    <thead>
    <tr>
        <th class="text_align_c">Место</th>
        <th class="text_align_c">И</th>
        <th class="text_align_c">В</th>
        <th class="text_align_c">Н</th>
        <th class="text_align_c">П</th>
        <th class="text_align_c">ГЗ</th>
        <th class="text_align_c">ГП</th>
        <th class="text_align_c">ГР</th>
        <th class="text_align_c">О</th>
    </tr>
    </thead>
    <tbody>
    <tr>
        <td class="text_align_c">{{table.position}}</td>
        <td class="text_align_c">{{table.played}}</td>
        <td class="text_align_c">{{table.won}}</td>
        <td class="text_align_c">{{table.draw}}</td>
        <td class="text_align_c">{{table.lost}}</td>
        <td class="text_align_c">{{table.scored}}</td>
        <td class="text_align_c">{{table.conceded}}</td>
        <td class="text_align_c">{{sub table.scored table.conceded}}</td>
        <td class="text_align_c">{{table.score}}</td>
    </tr>
    </tbody>
</table>