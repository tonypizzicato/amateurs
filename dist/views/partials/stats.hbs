{{#if stats.length}}<table class="table table_type_table s_m_0">{{#unless hideHeader}}<thead><tr><th class="col-md-05"><i class="fa fa-sort-numeric-asc"></i><th class="col-md-4 text_align_l text_overflow_ellipsis">Имя<th class="col-md-3 text_align_l">Клуб<th class="col-md-05 text_align_c">И<th class="col-md-05 text_align_c">Г<th class="col-md-05 text_align_c">П<th class="col-md-05 text_align_c text_overflow_ellipsis">Г+П<th class="col-md-05 text_align_c">ЖК<th class="col-md-05 text_align_c">КК<th class="col-md-05 text_align_c"><i class="fa fa-star"></i><th class="col-md-1 text_align_c">Очки{{/unless}}<tbody>{{#each stats}}<tr class="table__position" data-pos="{{inc @index}}"><td class="text_align_l">{{inc @index}}.<td class="text_align_l text_overflow_ellipsis">{{playerName}}<td class="text_align_l text_overflow_ellipsis"><span class="tl tl-{{get (last teams) '_id'}}-20 s_mr_10"></span>{{get (last teams) 'name'}}<td class="text_align_c">{{played}}<td class="text_align_c">{{goals}}<td class="text_align_c">{{assists}}<td class="text_align_c">{{add goals assists}}<td class="text_align_c">{{yellowCards}}<td class="text_align_c">{{redCards}}<td class="text_align_c">{{hearts_stars}}<td class="text_align_c">{{goals_assists_hearts_stars}}{{/each}}</table>{{else}}<p class="text_align_c s_pt_10">{{emptyText}}</p>{{/if}}