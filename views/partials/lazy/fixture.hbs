{{#notEmptyObject fixture}}
    {{#each fixture}}
        <h5 class="text_color_orange text_align_c s_mt_0">{{@key}} Тур</h5>

        <table class="table table_layout_fixed s_m_0 s_mb_20">
            <tbody>
            {{#each this}}
                <tr>
                    <td class="fixture__time col-md-3 text_size_12 text_color_light">
                        {{~#if dateTime}}{{datetime dateTime}}{{else}}Время не назначено{{/if~}}
                    </td>
                    <td class="fixture__team col-md-3 text_align_r s_pr_0
                        {{~#eq (gameResTeam teams.[0] this) 'w'}} fixture__winner{{/eq~}}
                        {{~#eq (gameResTeam teams.[0] this) 'l'}} fixture__looser{{/eq~}}
                                        ">
                        <a href="/{{../../league.slug}}/tournaments/{{../../tournament.slug}}/teams/{{teams.[0]._id}}">{{~teams.[0].name~}}</a>
                    <td class="col-md-1 text_align_c">
                        <a href="/{{../../league.slug}}/tournaments/{{../../tournament.slug}}/matches/{{_id}}">
                            {{#if score}}{{~score.ft.[0]}}:{{score.ft.[1]~}}{{else}}-:-{{/if}}
                        </a>
                    </td>
                    <td class="fixture__team col-md-3 text_align_l s_pl_0
                        {{~#eq (gameResTeam teams.[0] this) 'l'}} fixture__winner{{/eq~}}
                        {{~#eq (gameResTeam teams.[0] this) 'w'}} fixture__looser{{/eq~}}
                                        ">
                        <a href="/{{../../league.slug}}/tournaments/{{../../tournament.slug}}/teams/{{teams.[1]._id}}">{{~teams.[1].name~}}</a>
                    </td>
                    </td>

                    <td class="col-md-3 text_align_r text_size_12 text_color_light">
                        {{#if placeName}}
                            <a class="text_color_light" href="/{{../../../league.slug}}/tournaments/{{../../../tournament.slug}}/fields/{{slugify placeName}}">
                                {{placeName}}
                            </a>
                        {{/if}}
                    </td>
                </tr>
            {{/each}}
            </tbody>
        </table>
    {{/each}}
{{else}}
    <p class="text_align_c s_pt_10">{{emptyText}}</p>
{{/notEmptyObject}}
